.ph.section
  padding-top: 2rem
  padding-bottom: 4rem

  & + .ph.section
    padding-top: 0

.ph.svg
  height: 256px
  width: 256px
  display: block
  grid-area: svg

.ph.swatch
  display: grid
  grid-template-columns: 1fr 1fr
  grid-area: swatch
  margin-bottom: 4rem
  grid-gap: 0 0

  @media screen and (min-width: 425px)
    grid-template-columns: repeat(auto-fill, minmax(8rem, 1fr))

  @media screen and (min-width: 768px)
    grid-template-columns: repeat(auto-fit, minmax(8rem, 1fr))

  @media screen and (min-width: 1440px)
    grid-template-columns: repeat(auto-fit, minmax(8rem, 1fr))

  @media screen and (min-width: 2048px)
    grid-template-columns: repeat(auto-fill, minmax(8rem, 1fr))

  & .ph.color
    transition all 0.3s cubic-bezier(0.4, 0, 0.2, 1)
    width: auto
    aspect-ratio: 9 / 16
    height: auto
    text-transform: uppercase
    @media screen and (min-width: 2048px)
      max-width 12rem

    &::before
      text-transform: initial

.ph.accents
  display: grid
  grid-template-columns: 1fr 1fr
  grid-gap: 1rem
  grid-area: accents
  margin-bottom: 4rem

  @media screen and (min-width: 425px)
    grid-template-columns: repeat(auto-fill, minmax(8rem, 1fr))
    grid-gap: 1.5rem

  @media screen and (min-width: 768px)
    grid-template-columns: repeat(auto-fit, minmax(12rem, 1fr))
    grid-gap: 1.5rem

  @media screen and (min-width: 1440px)
    grid-template-columns: repeat(auto-fit, minmax(15rem, 1fr))
    grid-gap: 2rem

  & .ph.colors, & .ph.group
    display: grid
    justify-items: stretch
    grid-gap: 0

    & .ph.color
      text-transform: uppercase
      text-align: right
      min-height: 2rem
      font-weight: 500
      font-size: 1.25rem
      line-height: 2rem
      padding: 0.5rem
      font-family: var(--ph-font-family-code)

      &::before
        text-transform: initial

.ph.gradient
  display: block
  width: 100%
  height: 256px
