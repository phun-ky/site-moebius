    right: 1rem
    background-color: transparent
    border: none
    color: var(--ph-color-text)
    text-transform: uppercase
    font-size: 0.875rem

    &:hover
      color: var(--ph-color-text-accent)

[id*=-accents].ph
  & > .ph.group
    display: grid
    grid-template-columns: 1fr 1rem
    grid-template-areas: 'colors gradient'

    & .ph.colors
      grid-area: colors

    &::after
      grid-area: gradient
      transition all 0.3s cubic-bezier(0.4, 0, 0.2, 1)
      content: ''
      height: 100%
      width: 1rem

groups = interpolate luminanceShift monochromatic 'complement' 'split' triad tetrad pentad hexad analogous

accentGroupGradient()
