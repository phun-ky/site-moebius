    width: 100%
    height: max-content
    max-height: 90vh
    max-width: 425px

  @media screen and (min-width: 768px)
    max-width: 768px

  @media screen and (min-width: 1024px)
    width: 100%
    max-height: 80vh
    max-width: 1024px

::backdrop
  background-color: rgba(0, 0, 0, 0.55)

dialog.ph[open]
  display: flex
