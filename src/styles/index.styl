  & .ph.color-settings:last-of-type
    @media screen and (min-width: 2048px)
      margin-bottom: 0

.ph.container
  max-width: 1440px

  @media screen and (min-width: 2048px)
    max-width: 2048px
    width auto

  @media screen and (min-width: 4096px)
    max-width: 4096px

