input[readonly].ph
  background-color: transparent
  border-color: transparent
  padding-left: 0

input.ph.color-range
  position: relative
  z-index: 1
  appearance: none
  background-color: transparent
  border-radius: 1.5rem
  height: 1.5rem
  width: 100%
  display: block
  border: none
  outline: none
  margin-top: 0.75rem
  transition: color 0.05s linear
  background-image: linear-gradient(to right, rgb(255, 0, 0), rgb(255, 255, 0), rgb(0, 255, 0), rgb(0, 255, 255), rgb(0, 0, 255), rgb(255, 0, 255), rgb(255, 0, 0))

  &.lightness
    background-image: linear-gradient(to right, rgb(0, 0, 0), rgb(255, 255, 255))

  &.chroma
    background-image: linear-gradient(to right, rgb(255, 255, 255), rgb(217, 17, 240))

  &:focus
    outline: none

  &:active, &:hover:active
    cursor: grabbing

  &::-moz-range-track
    appearance: none
    opacity: 0
    outline: none !important

  &::-ms-track
    outline: none !important
    appearance: none
    opacity: 0

  &::-webkit-slider-thumb
    height: 1.5rem
    width: 1.5rem
    border-radius: 50%
    appearance: none
    background-color: var(--ph-color-background)
    border: 0.5rem solid var(--ph-color-background)
    outline: 0.25rem solid var(--ph-color-text-accent)
    transition: all 0.5s cubic-bezier(0.4, 0, 0.2, 1)
    box-shadow: inset 0 0 var(--ph-color-link)

    &:active, &:hover:active
      box-shadow: inset 2rem 2rem var(--ph-color-link)

    &:hover
      box-shadow: inset 2rem 2rem var(--ph-color-link)

  &::-moz-range-thumb
    height: 1.5rem
    width: 1.5rem
    border-radius: 50%
    appearance: none
    background-color: var(--ph-color-background)
    border: 0.5rem solid var(--ph-color-background)
    outline: 0.25rem solid var(--ph-color-text-accent)
    transition: all 0.5s cubic-bezier(0.4, 0, 0.2, 1)
    box-shadow: inset 0 0 var(--ph-color-link)

    &:active, &:hover:active
      box-shadow: inset 2rem 2rem var(--ph-color-link)

    &:hover
      box-shadow: inset 2rem 2rem var(--ph-color-link)

.ph.hue-map
  width: 100%
  margin-bottom: 2rem
  position: relative
  border-radius: 6px
  transition: background-image 0.3s cubic-bezier(0.4, 0, 0.2, 1), background-color 0.3s cubic-bezier(0.4, 0, 0.2, 1), box-shadow 0.5s cubic-bezier(0.4, 0, 0.2, 1)
  box-shadow: 0 16px 24px 2px rgba(0, 0, 0, 0.14), 0 6px 30px 5px rgba(0, 0, 0, 0.12), 0 8px 10px -5px rgba(0, 0, 0, 0.2)
  flex-grow: 1

  @media screen and (min-width: 425px)
    flex-grow: initial
    aspect-ratio: 16 / 9

  @media screen and (min-width: 768px)
    aspect-ratio: 32 / 9

  @media screen and (min-width: 1024px)
    aspect-ratio: 32 / 9

  &.primary
    background-image: linear-gradient(to top, transparent, black), linear-gradient(to right, white, var(--ph-hue-map-primary-gradient-color))

  &.secondary
    background-image: linear-gradient(to top, transparent, black), linear-gradient(to right, white, var(--ph-hue-map-secondary-gradient-color))

  &.divergent
    background-color: var(--ph-hue-map-divergent-background-color)

dialog.ph
  flex-direction: column
