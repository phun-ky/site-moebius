@css{
  :root{
    --ph-input-background-color: var(--ph-color-well-background);
    --ph-input-border-color: transparent;
    --moebius-primary-hue: 50;
    --moebius-primary-saturation: 100%;
    --moebius-primary-lightness: 50%;
    --moebius-secondary-hue: 50;
    --moebius-secondary-saturation: 100%;
    --moebius-secondary-lightness: 50%;
    --moebius-divergent-hue: 50;
    --moebius-divergent-saturation: 100%;
    --moebius-divergent-lightness: 50%;
    --ph-hue-map-primary-gradient-color: hsl(var(--moebius-primary-hue), var(--moebius-primary-saturation), var(--moebius-primary-lightness));
    --ph-hue-map-secondary-gradient-color: hsl(var(--moebius-secondary-hue), var(--moebius-secondary-saturation), var(--moebius-secondary-lightness));
    --ph-hue-map-divergent-background-color: hsl(var(--moebius-divergent-hue), var(--moebius-divergent-saturation), var(--moebius-divergent-lightness));
    --moebius-spacing-smallest: 0.25rem;
    --moebius-spacing-smaller: 0.5rem;
    --moebius-spacing-small: 1rem;
    --moebius-spacing-default: 1.5rem;
    --moebius-spacing-large: 2rem;
    --moebius-spacing-larger: 2.5rem;
    --moebius-spacing-largest: 4rem;
    --moebius-spacing-xxl: 6rem;
    --moebius-radius-smaller: 0.375rem;
    --moebius-radius-small: 0.5rem;
    --moebius-radius-large: 0.875rem;
    --moebius-radius-larger: 1.5rem;
    --ph-color-well-background: #262831;
    --ph-color-well-background-transparent: rgba(159, 168, 173,0.1);
    --ph-color-code-block-background: var(--ph-color-background);
  }

  :root.theme--light{
    --ph-color-well-background: #EEEEEE;
    --ph-input-background-color: var(--ph-color-well-background);
    --ph-input-border-color: rgba(51, 54, 56, 0.1);
    --ph-well-border-color: rgba(51, 54, 56, 0.1);
  }

  @media screen and (min-width: 425px){
      :root{
        --moebius-grid-gap: var(--moebius-spacing-small);
      }
    }
  @media screen and (min-width: 768px){
    :root{
      --moebius-grid-gap: var(--moebius-spacing-default);
    }
  }
  @media screen and (min-width: 1440px){
    :root{
      --moebius-grid-gap: var(--moebius-spacing-large);
    }
  }

}



.ph.tabs
  & .ph.tabs-list
    list-style: none
    display: flex
    flex-direction: column
    padding: 0
    margin: 0

    @media screen and (min-width: 768px)
      display: grid
      grid-template-columns: repeat(2, 1fr)
      grid-gap: 0

    @media screen and (min-width: 1200px)
      display: flex
      flex-direction: row
      flex-wrap: wrap
      grid-gap: var(--moebius-spacing-smallest)
      justify-content: flex-start
      align-items: bottom

.ph.tabs
  & .ph.tabs-list
    & .ph.tabs-list-action[aria-selected]
      color: var(--ph-color-text-accent)
      font-size: 1.75rem
      background-color: var(--ph-color-well-background)
      line-height: 100%
      border: 0.125rem solid var(--ph-input-border-color)
      border-bottom: none

      &::before
        @media screen and (min-width: 1200px)
          content: ''
          background-color: var(--ph-color-well-background)
          height: 0.125rem
          position: absolute
          top: 100%
          left: 0
          width: 100%
          display: block

      &::after
        content: '\0020â¬…'
        display: inline-block
        margin-left: 1ch

        @media screen and (min-width: 1200px)
          content: none
          margin-left: none
          display: none

.ph.tabs
  & .ph.tabs-list
    & .ph.tabs-list-action
      transition: background-color 0.5s cubic-bezier(0.4, 0, 0.2, 1)
      color: var(--ph-color-text)
      text-decoration: none
      font-size: 1rem
      line-height: 100%
      padding-bottom: 0.75rem
      padding-left: 0.75rem
      padding-right: 0.75rem
      padding-top: 0.75rem
      display: flex
      align-items: center
      justify-content: flex-start
      border: 0.125rem solid var(--ph-color-well-background)
      border-bottom: none
      flex-grow: 1

      @media screen and (min-width: 1200px)
        display: inline-flex
        align-items: flex-end
        flex-grow: 0
        background-color: rgba(38, 42, 59, 0.3)
        border-top-left-radius: 12px
        border-top-right-radius: 12px
        border: none

.ph.tabs
  & .ph.tabs-list
    & .ph.tabs-list-item
      padding: 0
      margin: 0
      min-height: 4rem
      display: flex
      flex-direction: column

      @media screen and (min-width: 1200px)
        flex-direction: row
        align-items: flex-end

.ph.tabs
  & section[id^='section'].ph
    background-color: var(--ph-color-well-background)
    border-bottom-right-radius: var(--moebius-radius-larger)
    border-bottom-left-radius: var(--moebius-radius-larger)
    padding: 2rem
    border: 0.125rem solid var(--ph-well-border-color)

    @media screen and (min-width: 1200px)
      border-top-right-radius: var(--moebius-radius-larger)

.ph.settings
