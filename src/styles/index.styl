  animation-timing-function: cubic-bezier(0.2, 0, 1, 0.9)
  animation-direction: forwards

@keyframes fadein
  0%
    opacity: 0
    transform: translate3d(0, 50%, 0)

  50%
    opacity: 0

  100%
    opacity: 1
    transform: translate3d(0, 0, 0)

